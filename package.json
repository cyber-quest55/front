{
  "name": "ant-design-pro",
  "version": "1.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "add-android": "cap add android",
    "add-ios": "cap add ios",
    "analyze": "cross-env ANALYZE=1 max build",
    "android": " max dev --android",
    "build": "max build",
    "build:dev": "cross-env REACT_APP_ENV=development UMI_ENV=development max build",
    "build:test": "cross-env REACT_APP_ENV=test UMI_ENV=test max build",
    "build:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=pre max build",
    "build:prod": "cross-env REACT_APP_ENV=production UMI_ENV=production max build",
    "build-android": "cross-env REACT_APP_ENV=development UMI_ENV=development max build  --android",
    "build-ios": "max build --ios",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "init": "cap init",
    "postinstall": "max setup",
    "ios": "max dev --ios",
    "jest": "jest",
    "lint": "npm run lint:js && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
    "openapi": "max openapi",
    "prepare": "husky install",
    "prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
    "preview": "npm run build  && max preview --port 8000",
    "record": "cross-env NODE_ENV=development REACT_APP_ENV=test max record --scene=login",
    "serve": "umi-serve",
    "start": "cross-env UMI_ENV=development max dev",
    "start:dev": "cross-env REACT_APP_ENV=development MOCK=none UMI_ENV=development max dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=development max dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=pre max dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=test max dev",
    "test": "jest",
    "test:coverage": "npm run jest -- --coverage",
    "test:update": "npm run jest -- -u",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/charts": "^1.4.2",
    "@ant-design/icons": "^4.8.0",
    "@ant-design/pro-components": "^2.4.4",
    "@ant-design/use-emotion-css": "1.0.4",
    "@capacitor-community/fcm": "^5.0.3",
    "@capacitor/android": "^5.1.1",
    "@capacitor/core": "^5.7.2",
    "@capacitor/geolocation": "^5.0.6",
    "@capacitor/ios": "^5.1.1",
    "@capacitor/push-notifications": "^5.1.1",
    "@diningcity/capacitor-qr-scanner": "^0.0.11",
    "@react-google-maps/api": "^2.18.1",
    "@react-hook/window-size": "^3.1.1",
    "@types/styled-system": "^5.1.16",
    "@umijs/route-utils": "^2.2.2",
    "ahooks": "^3.7.6",
    "antd": "^5.8.6",
    "antd-mask-input": "^2.0.7",
    "antd-mobile": "^5.32.0",
    "antd-mobile-icons": "^0.3.0",
    "axios": "^1.6.2",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.8",
    "design-system": "^2.5.0",
    "dva": "^2.4.1",
    "firebase": "^7.24.0",
    "lodash": "^4.17.21",
    "mockjs": "^1.1.0",
    "moment": "^2.29.4",
    "omit.js": "^2.0.2",
    "rc-menu": "^9.8.2",
    "rc-util": "^5.27.2",
    "react": "^18.2.0",
    "react-dev-inspector": "^1.8.4",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-helmet-async": "^1.3.0",
    "react-icons": "^4.8.0",
    "react-qr-reader": "^3.0.0-beta-1",
    "react-static-google-map": "^0.9.0",
    "react-svg": "^16.1.8",
    "socket.io-client": "^4.7.4",
    "spherical-geometry-js": "^3.0.0",
    "styled-system": "^5.1.5",
    "uniqid": "^5.4.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.5",
    "@capacitor/cli": "^5.1.1",
    "@testing-library/react": "^13.4.0",
    "@types/classnames": "^2.3.1",
    "@types/express": "^4.17.17",
    "@types/google-map-react": "^2.1.7",
    "@types/history": "^4.7.11",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-helmet": "^6.1.6",
    "@types/uniqid": "^5.3.2",
    "@umijs/fabric": "^2.14.1",
    "@umijs/lint": "^4.0.52",
    "@umijs/max": "^4.0.59",
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "express": "^4.18.2",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.8.4",
    "react-native": "^0.74.0",
    "swagger-ui-dist": "^4.15.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "umi-plugin-capacitor": "^0.0.9",
    "umi-presets-pro": "^2.0.2"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
